{
  "breakpoint": {
    "desktop": {
      "min": {
        "$type": "other",
        "$value": "1024px",
        "$description": "Desktop breakpoint minimum width"
      },
      "max": {
        "$type": "other",
        "$value": "9999px",
        "$description": "Desktop breakpoint maximum width"
      }
    }
  },
  "spacing": {
    "responsive": {
      "desktop": {
        "xs": {
          "$type": "spacing",
          "$value": "{spacing.base.2}",
          "$description": "Desktop extra small spacing"
        },
        "sm": {
          "$type": "spacing",
          "$value": "{spacing.base.4}",
          "$description": "Desktop small spacing"
        },
        "md": {
          "$type": "spacing",
          "$value": "{spacing.base.6}",
          "$description": "Desktop medium spacing"
        },
        "lg": {
          "$type": "spacing",
          "$value": "{spacing.base.8}",
          "$description": "Desktop large spacing"
        },
        "xl": {
          "$type": "spacing",
          "$value": "{spacing.base.10}",
          "$description": "Desktop extra large spacing"
        },
        "2xl": {
          "$type": "spacing",
          "$value": "{spacing.base.12}",
          "$description": "Desktop 2x large spacing"
        }
      }
    }
  },
  "size": {
    "responsive": {
      "desktop": {
        "font": {
          "xs": {
            "$type": "sizing",
            "$value": "0.75rem",
            "$description": "Desktop 12px font size"
          },
          "sm": {
            "$type": "sizing",
            "$value": "0.875rem",
            "$description": "Desktop 14px font size"
          },
          "base": {
            "$type": "sizing",
            "$value": "1rem",
            "$description": "Desktop 16px base font size"
          },
          "lg": {
            "$type": "sizing",
            "$value": "1.125rem",
            "$description": "Desktop 18px font size"
          },
          "xl": {
            "$type": "sizing",
            "$value": "1.25rem",
            "$description": "Desktop 20px font size"
          },
          "2xl": {
            "$type": "sizing",
            "$value": "1.5rem",
            "$description": "Desktop 24px font size"
          },
          "3xl": {
            "$type": "sizing",
            "$value": "1.875rem",
            "$description": "Desktop 30px font size"
          },
          "4xl": {
            "$type": "sizing",
            "$value": "2.25rem",
            "$description": "Desktop 36px font size"
          },
          "5xl": {
            "$type": "sizing",
            "$value": "3rem",
            "$description": "Desktop 48px font size"
          },
          "6xl": {
            "$type": "sizing",
            "$value": "3.75rem",
            "$description": "Desktop 60px font size"
          },
          "7xl": {
            "$type": "sizing",
            "$value": "4.5rem",
            "$description": "Desktop 72px font size"
          },
          "8xl": {
            "$type": "sizing",
            "$value": "6rem",
            "$description": "Desktop 96px font size"
          },
          "9xl": {
            "$type": "sizing",
            "$value": "8rem",
            "$description": "Desktop 128px font size"
          }
        }
      }
    }
  },
  "typography": {
    "responsive": {
      "desktop": {
        "display": {
          "large": {
            "$type": "typography",
            "$value": {
              "fontFamily": "{typography.reference.display.fontFamily}",
              "fontWeight": "{font.weight.black}",
              "fontSize": "{size.responsive.desktop.font.9xl}",
              "lineHeight": "{typography.reference.display.lineHeight}",
              "letterSpacing": "{typography.reference.display.letterSpacing}",
              "paragraphSpacing": "{typography.reference.display.paragraphSpacing}",
              "textCase": "{typography.reference.display.textCase}",
              "textDecoration": "{typography.reference.display.textDecoration}"
            },
            "$description": "Desktop large display text"
          },
          "medium": {
            "$type": "typography",
            "$value": {
              "fontFamily": "{typography.reference.display.fontFamily}",
              "fontWeight": "{font.weight.extrabold}",
              "fontSize": "{size.responsive.desktop.font.6xl}",
              "lineHeight": "{typography.reference.display.lineHeight}",
              "letterSpacing": "{typography.reference.display.letterSpacing}",
              "paragraphSpacing": "{typography.reference.display.paragraphSpacing}",
              "textCase": "{typography.reference.display.textCase}",
              "textDecoration": "{typography.reference.display.textDecoration}"
