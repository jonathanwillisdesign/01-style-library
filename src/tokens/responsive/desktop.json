              "fontWeight": "{font.weight.semibold}",
              "fontSize": "{size.responsive.desktop.font.xl}",
              "lineHeight": "{font.lineHeight.normal}",
              "letterSpacing": "{typography.reference.heading.letterSpacing}",
              "paragraphSpacing": "{typography.reference.heading.paragraphSpacing}",
              "textCase": "{typography.reference.heading.textCase}",
              "textDecoration": "{typography.reference.heading.textDecoration}"
